use '@std/util';

@funcTools <- skip;
func print() __builtin_print(this <- arguments, '');

@funcTools <- skip;
func println() __builtin_print(this <- arguments, '\n');

@funcTools <- skip;
func readline() __builtin_readline(this <- arguments);
