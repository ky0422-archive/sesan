let x = 1;

while (x < 6) {
    if (x >= 3) 
        println('x (' + to_s(x) + ') is greater than or equal to 5')
    else
        println('x (' + to_s(x) + ') is less than 5')

    x = x + 1;
}

if (x == 1) println('x is 1')
else if (x == 2) println('x is 2')
else if (x == 3) println('x is 3')
else if (x == 4) println('x is 4')
else if (x == 5) println('x is 5')
else {
    println('x is greater than 5');
    println('x is', x);
}
