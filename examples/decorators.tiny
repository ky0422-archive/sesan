let skip = { skipCheckArguments: true };
let custom = { customDecorator: 'hello' };

@skip + custom
func foo() {
    println(this <- decorator <- customDecorator);

    let x = join(this <- arguments, ' ');

    @{ capture: true }
    func ret() {
        println(x);
    };

    return ret;
};

foo(1, 2, 3, 'Hello')();
