{
    "version": 2,
    "public": true,
    "builds": [
        {
            "src": "api.ts",
            "use": "@vercel/node"
        },
        {
            "src": "./@std/array.tiny",
            "use": "@vercel/static"
        },
        {
            "src": "./@std/io.tiny",
            "use": "@vercel/static"
        },
        {
            "src": "./@std/util.tiny",
            "use": "@vercel/static"
        },
        {
            "src": "./@std/lib.tiny",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api.ts"
        }
    ]
}
