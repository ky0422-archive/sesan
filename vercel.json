{
    "version": 2,
    "public": true,
    "builds": [
        {
            "src": "/core/api.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["@std/**", "examples/function.tiny"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/core/api.ts"
        }
    ]
}
