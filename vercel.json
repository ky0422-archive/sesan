{
    "version": 2,
    "public": true,
    "builds": [
        {
            "src": "api.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api.ts"
        }
    ],
    "functions": [
        {
            "api.ts": {
                "includeFiles": ["**/*.tiny"]
            }
        }
    ]
}
