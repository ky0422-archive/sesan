let a = 20;

func test(a) {
    println('test >', a, 5); // test > 5
    a = 30;
    println('test >', a, 30); // test > 30

    func test2(a) {
        println('test2 >', a, 12); // test2 > 12
        a = 40;
        println('test2 >', a, 40); // test2 > 40
    }

    test2(12);

    println('test >', a, 30); // test > 30
}

test(5);
println(a, 20); // 20
